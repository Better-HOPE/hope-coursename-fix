{
    "name": "hope-coursename-fix",
    "version": "0.0.3",
    "manifest_version": 3,
    "description": "Fix course names in hope",
    "content_scripts": [
        {
            "matches": [
                "https://hope.fun.ac.jp/*"
            ],
            "run_at": "document_end",
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [{
        "resources": ["course-list.json"],
        "matches": ["https://hope.fun.ac.jp/*"]
    }]
}
